{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "deploy": "git push heroku master",
    "dev": "webpack -w",
    "git": "npm run build && git add --all && git commit -am \"$(date)\"",
    "major": "npm run git && npm version major && npm run push",
    "minor": "npm run git && npm version minor && npm run push",
    "patch": "npm run git && npm version patch && npm run push",
    "push": "git push origin master --tags",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nemanja Stojanovic",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.12.0",
    "express": "^4.13.4",
    "foundation-sites": "^6.2.3",
    "jquery": "^3.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  }
}
